    <Sequence.Variables>
      <Variable x:TypeArguments="x:Double" sap2010:Annotation.AnnotationText="画像の幅比率 = &#xA;  読み込み画像の幅 / 設定値（OCR対象画像の幅）" Name="ratioWidth" />
